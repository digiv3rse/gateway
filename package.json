  "scripts": {
    "build": "webpack --mode production && node scripts/copyNodeModules.js",
    "cypress": "cypress",
    "cypress:ci": "cypress run",
    "e2e:server": "yarn mock-server & yarn start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "mock-server": "node util/mock-server.js",
    "riffraff": "npx @guardian/node-riffraff-artifact",
    "start": "node ./build/server.js",
    "test": "yarn type-check && yarn test:unit",
    "test:unit": "jest --ci",
    "type-check": "tsc --noEmit --skipLibCheck",
    "watch:server": "nodemon ./build/server.js",
    "watch": "webpack --watch",
    "wait-on:server": "wait-on http://localhost:$PORT/healthcheck",
    "wait-on:mock-server": "wait-on http://localhost:9000/healthcheck"
  },
